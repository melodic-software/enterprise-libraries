<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Enterprise MediatR</Title>
    <Description></Description>
    <PackageTags></PackageTags>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MediatR" Version="12.2.0" />
    <PackageReference Include="Melodic.Enterprise.ApplicationServices" Version="1.0.0-preview.35" />
    <PackageReference Include="Melodic.Enterprise.Caching" Version="1.0.0-preview.41" />
    <PackageReference Include="Melodic.Enterprise.DomainDrivenDesign" Version="1.0.0-preview.48" />
    <PackageReference Include="Melodic.Enterprise.Events" Version="1.0.0-preview.45" />
    <PackageReference Include="Melodic.Enterprise.Exceptions" Version="1.0.0-preview.31" />
    <PackageReference Include="Melodic.Enterprise.FluentValidation" Version="1.0.0-preview.32" />
    <PackageReference Include="Melodic.Enterprise.Options" Version="1.0.0-preview.32" />
    <PackageReference Include="Melodic.Enterprise.Reflection" Version="1.0.0-preview.33" />
  </ItemGroup>

</Project>