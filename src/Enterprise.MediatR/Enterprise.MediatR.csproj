<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Enterprise MediatR</Title>
    <Description></Description>
    <PackageTags></PackageTags>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MediatR" Version="12.3.0" />
    <PackageReference Include="Melodic.Enterprise.ApplicationServices" Version="1.0.0-preview.523" />
    <PackageReference Include="Melodic.Enterprise.Caching" Version="1.0.0-preview.522" />
    <PackageReference Include="Melodic.Enterprise.DomainDrivenDesign" Version="1.0.0-preview.523" />
    <PackageReference Include="Melodic.Enterprise.FluentValidation" Version="1.0.0-preview.497" />
    <PackageReference Include="Melodic.Enterprise.Options" Version="1.0.0-preview.522" />
  </ItemGroup>

</Project>