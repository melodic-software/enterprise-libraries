<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Enterprise Patterns - Outbox</Title>
    <Description></Description>
    <PackageTags></PackageTags>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Melodic.Enterprise.DateTimes" Version="1.0.0-preview.73" />
    <PackageReference Include="Melodic.Enterprise.Events" Version="1.0.0-preview.79" />
    <PackageReference Include="Melodic.Enterprise.Serialization.Json" Version="1.0.0-preview.16" />
  </ItemGroup>

</Project>