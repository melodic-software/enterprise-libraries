<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Title>Enterprise Options</Title>
    <Description></Description>
    <PackageTags></PackageTags>
    <NoWarn>$(NoWarn);1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>$(AssemblyName).xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Melodic.Enterprise.Library.Core" Version="1.0.0-preview.563" />
    <PackageReference Include="Melodic.Enterprise.Options.Core" Version="1.0.0-preview.397" />
    <PackageReference Include="Melodic.Enterprise.Reflection" Version="1.0.0-preview.544" />
    <PackageReference Include="Melodic.Enterprise.Serialization.Json" Version="1.0.0-preview.561" />
  </ItemGroup>

</Project>